<?xml version="1.0" encoding="UTF-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="SwissDeductions.Views.IncomePage">
    <ContentPage.Content>
        <Label FontSize="35"
        Text="What is your gross annual income for year "/>
        <Picker x:Name="picker" Title="Select a year">
            <Picker.ItemsSource>
                <x:Array Type="{x:Type x:String}">
                    <x:String>2018</x:String>
                    <x:String>2019</x:String>
                    <x:String>2020</x:String>
                    <x:String>2021</x:String>
                    <x:String>2022</x:String>
                </x:Array>
            </Picker.ItemsSource>
        </Picker>
        <Entry Placeholder="Annual Income"/>
        <Button Clicked="Handle_Clicked"
        Text="Next"/>
    </ContentPage.Content>
</ContentPage>
